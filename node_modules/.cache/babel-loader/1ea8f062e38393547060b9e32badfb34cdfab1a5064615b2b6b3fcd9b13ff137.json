s = JSON.parse(localStorage.getItem('notes'));\n      if (this.celectedCategory != 'all' && this.celectedCategory.length) {\n         return this.notes\n      } else {\n        for (let i = 0; i < this.notes.length; i++ ) {\n          //console.log(this.notes[i].cat);\n          if (this.celectedCategory === this.notes[i].cat) {\n            this.notes.splice(i, 1)\n          } \n        } \n      }\n      this.$emit('sendFiltCategoty', this.notes);\n      console.log(this.notes);\n      console.log(this.celectedCategory);\n\n\n    }\n  }\n}\n</script>\n\n\n"],"mappings":"AAaA;EACAA,IAAA;EACAC,KAAA;EACAC,IAAA,WAAAA,CAAA;IACA;MACAC,KAAA;MACAC,gBAAA;IACA;EACA;EACAC,QAAA;IACAC,WAAA;MACA,YAAAC,MAAA,CAAAC,OAAA;IACA;EACA;EACAC,OAAA;IACAC,eAAA;MACA,KAAAP,KAAA,GAAAQ,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACA,SAAAV,gBAAA,kBAAAA,gBAAA,CAAAW,MAAA;QACA,YAAAZ,KAAA;MACA;QACA,SAAAa,CAAA,MAAAA,CAAA,QAAAb,KAAA,CAAAY,MAAA,EAAAC,CAAA;UACA;UACA,SAAAZ,gBAAA,UAAAD,KAAA,CAAAa,CAAA,EAAAC,GAAA;YACA,KAAAd,KAAA,CAAAe,MAAA,CAAAF,CAAA;UACA;QACA;MACA;MACA,KAAAG,KAAA,0BAAAhB,KAAA;MACAiB,OAAA,CAAAC,GAAA,MAAAlB,KAAA;MACAiB,OAAA,CAAAC,GAAA,MAAAjB,gBAAA;IAGA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}