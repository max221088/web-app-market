ories'];\n        },\n    },\n  methods: {\n    filtByCategory () {\n      this.notes = JSON.parse(localStorage.getItem('notes'));\n      if (this.celectedCategory != 'all') {\n        for (let i = 0; i < this.notes.length; i++ ) {\n          //console.log(this.notes[i].cat);\n          if (this.celectedCategory != this.notes[i].cat) {\n            this.filtNotes.push(this.notes[i]);\n          } \n        } \n      } else {\n        this.filtNotes = JSON.parse(localStorage.getItem('notes'));\n      }\n      console.log(this.filtNotes);\n      this.$emit('sendFiltCategoty', this.filtNotes);\n    }\n  }\n}\n</script>\n\n\n"],"mappings":";AAaA;EACAA,IAAA;EACAC,KAAA;EACAC,IAAA,WAAAA,CAAA;IACA;MACAC,KAAA;MACAC,SAAA;MACAC,gBAAA;IACA;EACA;EACAC,QAAA;IACAC,WAAA;MACA,YAAAC,MAAA,CAAAC,OAAA;IACA;EACA;EACAC,OAAA;IACAC,eAAA;MACA,KAAAR,KAAA,GAAAS,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACA,SAAAV,gBAAA;QACA,SAAAW,CAAA,MAAAA,CAAA,QAAAb,KAAA,CAAAc,MAAA,EAAAD,CAAA;UACA;UACA,SAAAX,gBAAA,SAAAF,KAAA,CAAAa,CAAA,EAAAE,GAAA;YACA,KAAAd,SAAA,CAAAe,IAAA,MAAAhB,KAAA,CAAAa,CAAA;UACA;QACA;MACA;QACA,KAAAZ,SAAA,GAAAQ,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACA;MACAK,OAAA,CAAAC,GAAA,MAAAjB,SAAA;MACA,KAAAkB,KAAA,0BAAAlB,SAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}